/* components/calendar2/index.wxss */
.box1 .dateBox{
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  margin-top: 20px;
  font-size: 40rpx;
}

.box1{
  display: flex;
  flex-wrap: wrap;
  margin: 0 auto;
}

.box1>view{
  height: 30px;
  line-height: 30px;
  text-align: center;
  font-size: 34rpx;
}

.dateStart{
  background-color: hotpink;
  color: #fff;
  border-top-left-radius: 50%; 
  border-bottom-left-radius: 50%; 
}
.dateEnd{
  background-color: hotpink;
  color: #fff;
  border-top-right-radius: 50%; 
  border-bottom-right-radius: 50%; 
}
.dateSelect{
  background-color: hotpink;
  color: #fff;
   
}
.flex{
  display: flex;
  justify-content: space-between;
  width:100%;
}
.flex-list{
  flex: 1;
}
.flex-btn{
  width: 100rpx
}
.list-m{
  margin-top: 8px;
}