<!--pages/components/map/index.wxml-->
<view class='map-page'>
   <map longitude='{{mapData.longitude}}'
       latitude="{{mapData.latitude}}"
       markers='{{mapData.markers}}'
       covers='{{mapData.covers}}'
       scale="{{mapData.scale}}"
       show-location="{{mapData.showLocation}}"style="width:100%;height:200px"></map> 
</view>